#include <stdio.h>
int main()
{
    int n1, n2, minm;
    printf("Enter two positive integers: ");
    scanf("%d %d", &n1, &n2);
    minm = (n1>n2) ? n1 : n2;
    while(1)
    {
        if( minm%n1==0 && minm%n2==0 )
        {
            printf("The LCM of %d and %d is %d.", n1, n2,minm);
            break;
        }
        ++minm;
    }
    return 0;
}
